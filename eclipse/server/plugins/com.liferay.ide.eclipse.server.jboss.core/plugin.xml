<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.wst.server.core.runtimeTypes">
      <runtimeType
            class="com.liferay.ide.eclipse.server.jboss.core.LiferayLocalJBoss7ServerRuntime"
            description="Liferay Portal 6.1 CE bundled with JBoss AS7"
            id="com.liferay.ide.eclipse.server.jboss.runtime.70"
            name="Liferay v6.1 CE (JBoss AS7)"
            vendor="Liferay, Inc."
            version="7.0">
        <moduleType types="jst.appclient" versions="5.0, 6.0"/>
          <moduleType types="wst.web" versions="1.0,1.2,1.3,1.4"/>
          <moduleType types="jst.web" versions="2.2, 2.3, 2.4, 2.5, 3.0"/>
          <moduleType types="jst.ejb" versions="1.0, 1.1, 2.0, 2.1, 3.0, 3.1"/>
          <moduleType types="jst.ear" versions="1.2, 1.3, 1.4, 5.0, 6.0"/>
          <moduleType types="jst.connector" versions="1.0, 1.5, 1.6"/>
	      <moduleType types="jst.utility" versions="1.0"/>
          <moduleType types="jboss.package" versions="1.0"/>
          <moduleType types="jboss.singlefile" versions="1.0"/>
          <moduleType types="jst.jboss.esb" versions="4.2,4.3,4.4,4.5,4.6,4.7,4.9"/>
          <moduleType types="jst.jboss.sar" versions="1.0"/>
          <moduleType types="jbt.bpel.module" versions="1.1, 2.0"/>
          <moduleType types="bpel.module" versions="1.1, 2.0"/>
          <moduleType types="jboss.osgi" versions="1.0"/>
      </runtimeType>
   </extension>
   <extension
         point="org.eclipse.jst.server.core.runtimeClasspathProviders">
      <runtimeClasspathProvider
            class="com.liferay.ide.eclipse.server.jboss.core.LiferayBossRuntimeClasspathProvider"
            id="com.liferay.ide.eclipse.server.jboss.7x.runtimeClasspathProvider"
            runtimeTypeIds="com.liferay.ide.eclipse.server.jboss.runtime.70">
      </runtimeClasspathProvider>
   </extension>
   <extension
         point="org.eclipse.wst.server.core.serverTypes">
      <serverType
            behaviourClass="com.liferay.ide.eclipse.server.jboss.core.LiferayJBossServerBehavior"
            class="com.liferay.ide.eclipse.server.jboss.core.LiferayJBoss7Server"
            description="Liferay Portal 6.1 Community Edition Server bundled with JBoss AS7"
            hasConfiguration="false"
            id="com.liferay.ide.eclipse.server.jboss.70"
            initialState="stopped"
            launchConfigId="org.jboss.ide.eclipse.as.core.server.JBoss7StartupConfiguration"
            launchModes="run,debug,profile"
            name="Liferay v6.1 CE Server (JBoss AS7)"
            runtime="true"
            runtimeTypeId="com.liferay.ide.eclipse.server.jboss.runtime.70"
            startBeforePublish="false"
            startTimeout="450000"
            stopTimeout="450000"
            supportsRemoteHosts="false">
      </serverType>
   </extension>
   <extension
         point="org.eclipse.wst.common.project.facet.core.runtimes">
      <runtime-component-type
            id="com.liferay.ide.eclipse.server.jboss.runtime-component">
      </runtime-component-type>
      <runtime-component-version
            type="com.liferay.ide.eclipse.server.jboss.runtime-component"
            version="7.0">
      </runtime-component-version>
      <supported>
         <runtime-component
               id="com.liferay.ide.eclipse.server.jboss.runtime-component"
               version="7.0"></runtime-component>
           <facet id="wst.web"  version="1.0,1.2,1.3,1.4"/>
       <facet id="jst.web"  version="2.2,2.3,2.4,2.5,3.0"/>
       <facet id="jst.java" version="5.0,6.0,1.7"/>
       <facet id="jst.utility" version="1.0"/>
       <facet id="jst.connector" version="1.0,1.5,1.6"/>
       <facet id="jst.ejb" version="2.0,2.1,3.0,3.1"/>
       <facet id="jst.ear" version="1.2,1.3,1.4,5.0,6.0"/>
       <facet id="jst.appclient" version="1.2,1.3,1.4,5.0,6.0"/>
       <facet id="jst.webfragment" version="3.0"/>
         <facet
               id="liferay.portlet"
               version="6.0">
         </facet>
         <facet
               id="liferay.hook"
               version="6.0">
         </facet>
         <facet
               id="liferay.ext"
               version="6.0">
         </facet>
         <facet
               id="liferay.layouttpl"
               version="6.0">
         </facet>
         <facet
               id="liferay.theme"
               version="6.0">
         </facet>      
      </supported>
      <adapter>
         <runtime-component
               id="com.liferay.ide.eclipse.server.jboss.runtime-component"
               version="7.0"></runtime-component>
         <factory
               class="com.liferay.ide.eclipse.server.core.RuntimeClasspathFactory">
         </factory>
         <type
               class="org.eclipse.jst.common.project.facet.core.IClasspathProvider">
         </type>
      </adapter>
   </extension>
   <extension
         point="org.eclipse.jst.server.core.runtimeFacetMappings">
      <runtimeFacetMapping
            runtime-component="com.liferay.ide.eclipse.server.jboss.runtime-component"
            runtimeTypeId="com.liferay.ide.eclipse.server.jboss.runtime.70"
            version="7.0">
      </runtimeFacetMapping>
   </extension>
   <extension
         point="com.liferay.ide.eclipse.server.core.runtimeStubs">
      <liferayRuntimeStub
            name="Liferay v6.1 CE (JBoss AS7) Bundle"
            runtimeTypeId="com.liferay.ide.eclipse.server.jboss.runtime.70">
      </liferayRuntimeStub>
   </extension>
   <extension
         point="org.eclipse.wst.server.core.launchableAdapters">
      <launchableAdapter
            class="org.jboss.ide.eclipse.as.core.server.internal.JBossLaunchAdapter"
            id="com.liferay.ide.eclipse.server.jboss.web"
            typeIds="com.liferay.ide.eclipse.server.jboss.70">
      </launchableAdapter>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            category="JBoss"
            delegate="com.liferay.ide.eclipse.server.jboss.core.internal.LiferayJBossStartLaunchConfiguration"
            id="com.liferay.ide.eclipse.server.jboss.70.launchConfigurationType"
            modes="run,debug,profile"
            name="Liferay v6.1 CE (JBoss AS7)"
            public="false"
            sourceLocatorId="org.eclipse.jdt.launching.sourceLocator.JavaSourceLookupDirector"
            sourcePathComputerId="org.eclipse.jst.server.generic.core.sourcePathComputer">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.jboss.ide.eclipse.as.core.behaviourExtension">
      <behaviour
            behaviourDelegate="org.jboss.ide.eclipse.as.core.server.internal.v7.LocalJBoss7BehaviorDelegate"
            launchDelegate="com.liferay.ide.eclipse.server.jboss.core.internal.LiferayJBoss7StartLaunchDelegate"
            name="Local"
            publishMethod="org.jboss.ide.eclipse.as.core.publishers.LocalPublishMethod"
            serverTypes="com.liferay.ide.eclipse.server.jboss.70"
            typeId="local">
      </behaviour>
   </extension>
   <extension
         point="org.jboss.ide.eclipse.as.core.pollers">
      <serverPoller
            class="com.liferay.ide.eclipse.server.jboss.core.internal.LiferayJBoss7ManagerServicePoller"
            id="com.liferay.ide.eclipse.server.jboss.core.internal.LiferayJBoss7ManagerServicePoller"
            name="Liferay JBoss7 Manager Service"
            serverTypes="com.liferay.ide.eclipse.server.jboss.70"
            supportsShutdown="true"
            supportsStartup="true">
      </serverPoller>
   </extension>
 

</plugin>
