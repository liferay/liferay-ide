<?xml version="1.0"?>
<?eclipse version="3.4"?>
<plugin>

	<extension
		point="org.eclipse.wst.common.project.facet.ui.images"
	>

		<image
			category="liferay.plugins"
			facet="liferay.layouttpl"
			path="icons/e16/layout.png"
		>
		</image>

	</extension>
	<extension
		point="org.eclipse.ui.editors"
	>
		<editor
			class="com.liferay.ide.layouttpl.ui.editor.LayoutTplEditor"
			default="true"
			extensions="tpl"
			icon="icons/e16/layout.png"
			id="com.liferay.ide.eclipse.layouttpl.ui.template.multiEditor"
			name="%layout.editor.name"
		>
		</editor>
	</extension>
</plugin>