	<extension
		point="org.eclipse.ui.popupMenus"
	>
		<objectContribution
			adaptable="true"
			id="com.liferay.ide.project.ui.addLiferayNature"
			objectClass="org.eclipse.core.resources.IProject"
		>
			<action
				class="com.liferay.ide.project.ui.action.AddLiferayNatureAction"
				icon="icons/e16/liferay_logo_16.png"
				id="com.liferay.ide.project.ui.action.AddLiferayNatureAction"
				label="Add Liferay Project Nature"
				menubarPath="org.eclipse.ui.projectConfigure/additions"
			>
			</action>
			<visibility>
				<and>
					<objectState name="open" value="true" />
					<or>
						<objectState name="nature" value="org.eclipse.m2e.core.maven2Nature" />
						<objectState name="nature" value="org.eclipse.buildship.core.gradleprojectnature" />
					</or>
					<not>
						<objectState name="nature" value="com.liferay.ide.core.liferayNature" />
					</not>
				</and>
			</visibility>
		</objectContribution>
		<objectContribution
			adaptable="true"
			id="com.liferay.ide.ui.popupmenu"
			objectClass="org.eclipse.core.resources.IResource"
		>
			<menu
				icon="icons/liferay_logo_16.png"
				id="liferayPopupMenuCategory"
				label="%liferay.menu.label"
			>
				<separator name="slot1" />
			</menu>
		</objectContribution>
		<objectContribution
			adaptable="true"
			id="org.eclipse.compare.ext.objectContribution"
			objectClass="org.eclipse.core.resources.IResource"
		>
			<action
				class="com.liferay.ide.project.ui.action.CompareOriginalImplementationAction"
				enablesFor="1"
				id="com.liferay.ide.project.ui.action.compareOriginalImplementation"
				label="%compare.with.action.label"
				menubarPath="compareWithMenu/compareWithGroup"
			>
			</action>
			<enablement>
				<adapt type="org.eclipse.core.resources.IResource">
					<or>
						<adapt type="org.eclipse.core.resources.IFile">
							<test
								property="com.liferay.ide.project.ui.isModuleFragmentProject"
							/>
						</adapt>
						<adapt type="org.eclipse.core.resources.IFile">
							<test
								property="com.liferay.ide.project.ui.isModuleExtProject"
							/>
						</adapt>
					</or>
				</adapt>
			</enablement>
		</objectContribution>
	</extension>

