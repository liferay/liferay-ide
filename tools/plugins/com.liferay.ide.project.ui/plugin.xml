			class="com.liferay.ide.project.ui.LiferayProjectPropertyPage"
			id="com.liferay.ide.project.ui.properties"
			name="%liferay.page.name"
		>
			<enabledWhen>
				<adapt
					type="org.eclipse.core.resources.IProject"
				>
					<test
						property="com.liferay.ide.project.ui.isLiferayProject"
					>
					</test>
				</adapt>
			</enabledWhen>
		</page>
	</extension>
	<extension
		point="org.eclipse.ui.propertyPages"
	>
		<page
			category="com.liferay.ide.project.ui.properties"
			class="com.liferay.ide.project.ui.pref.PluginValidationSettingsPage"
			id="com.liferay.ide.project.ui.propertyPage.validation"
			name="%plugin.validation.page.name"
		>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject">
					<test property="com.liferay.ide.project.ui.isLiferayProject">
					</test>
				</adapt>
			</enabledWhen>
		</page>
	</extension>

