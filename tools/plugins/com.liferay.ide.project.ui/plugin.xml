			id="com.liferay.ide.project.ui.modules.NewLiferayComponentWizard"
			name="Liferay Component Class"
		>
			<class class="com.liferay.ide.project.ui.modules.NewLiferayComponentWizard">
				<parameter name="liferay_artifact" value="true" />
				<parameter name="menuIndex" value="1" />
			</class>
