	</extension>
	<extension
		 point="org.eclipse.ui.popupMenus">
		<objectContribution
			adaptable="true"
			id="com.liferay.ide.server.ui.popup.server"
			objectClass="org.eclipse.wst.server.core.IServer">
			<visibility>
				<and>
					<objectState
						name="serverType"
					value="com.liferay.*">
					</objectState>
				</and>
			</visibility>
			<action
				class="com.liferay.ide.server.ui.action.TestWebServicesAction"
				enablesFor="1"
				icon="icons/e16/test_web_services_16x16.png"
				id="com.liferay.ide.server.ui.action.testWebServices.server"
			   label="%test.services.action.label">
			</action>
			<action
				class="com.liferay.ide.server.ui.action.OpenLiferayHomeFolderServerAction"
				definitionId="com.liferay.ide.server.ui.handlers.openLiferayHomeFolder"
				enablesFor="1"
				icon="icons/liferay_logo_16.png"
				id="com.liferay.ide.server.ui.action.openLiferayHomeFolderServerAction"
			   label="%open.liferay.home.folder.action.label">
			</action>
			<action
				definitionId="com.liferay.ide.server.ui.handlers.openjsonwsapi"
				class="com.liferay.ide.server.ui.action.OpenJSONWSAPIAction"
				enablesFor="1"
				icon="icons/e16/test_web_services_16x16.png"
				id="com.liferay.ide.server.ui.action.openJSONWSAPIAction"
			   label="%test.json.services.action.label">
			</action>
			<action
				definitionId="com.liferay.ide.server.ui.handlers.openportalhome"
				class="com.liferay.ide.server.ui.action.OpenPortalHomeAction"
				enablesFor="1"
				icon="icons/e16/open_portal_16x16.png"
				id="com.liferay.ide.server.ui.action.openPortalHomeAction"
			   label="%open.portal.home.action.label">
			</action>
			<action
				class="com.liferay.ide.server.ui.action.CreatePortalSettingsFileAction"
				enablesFor="1"
				icon="icons/e16/properties.png"
				id="com.liferay.ide.server.ui.action.createPortalSettingsFile"
			   label="Create Portal Settings File">
			</action>
		</objectContribution>
		<objectContribution
			adaptable="true"
			id="com.liferay.ide.server.ui.popup.dbconnection"
			objectClass="org.eclipse.wst.server.core.IServer">
			<visibility>
				<and>
					<objectState
						name="serverType"
					  value="com.liferay.*">
					</objectState>
				</and>
			</visibility>
			<action
				class="com.liferay.ide.server.ui.action.CreateDBConnectAction"
				enablesFor="1"
				icon="icons/e16/jdbc_16.gif"
				id="com.liferay.ide.server.ui.action.createDBConnectAction"
			   label="%create.database.connection.action.label">
			</action>
		</objectContribution>
		<objectContribution
			adaptable="true"
			id="com.liferay.ide.server.ui.popup.module"
			objectClass="org.eclipse.wst.server.ui.IServerModule">
			<visibility>
				<and>
					<objectState
						name="serverType"
					value="com.liferay.*">
					</objectState>
				</and>
			</visibility>
			<action
				class="com.liferay.ide.server.ui.action.TestWebServicesAction"
				enablesFor="1"
				icon="icons/e16/test_web_services_16x16.png"
				id="com.liferay.ide.server.ui.action.testWebServices.plugin"
			   label="%test.services.action.label">
			</action>
			<action
				definitionId="com.liferay.ide.server.ui.handlers.redeploy"
				class="com.liferay.ide.server.ui.action.RedeployAction"
				enablesFor="1"
				icon="icons/e16/redeploy_16x16.gif"
				id="com.liferay.ide.server.ui.action.redeploy"
			   label="%redeploy.action.label">
			</action>
			<action
				definitionId="com.liferay.ide.server.ui.handlers.opendeployedfolder"
				class="com.liferay.ide.server.ui.action.OpenDeployedFolderAction"
				enablesFor="1"
				icon="icons/e16/open_deployed_folder.gif"
				id="com.liferay.ide.server.ui.action.opendeployed"
			   label="Open Deployed Folder">
			</action>
		</objectContribution>
		<objectContribution
			adaptable="true"
			id="com.liferay.ide.server.ui.folder.popup.addServer"
			objectClass="org.eclipse.core.resources.IFolder">
			<enablement>
				<adapt type="org.eclipse.core.resources.IFolder">
					<and>
						<test forcePluginActivation="true"
				  property="com.liferay.ide.server.ui.canCreateNewLiferayServer"
				  value="true" />
					</and>
				</adapt>
			</enablement>
			<action
				class="com.liferay.ide.server.ui.action.CreateNewLiferayServerAction"
				enablesFor="1"
				icon="icons/e16/server_new.png"
				id="com.liferay.ide.server.ui.action.createNewLiferayServerFromLocation"
			   label="Create New Liferay Server from location">
			</action>
		</objectContribution>
		<objectContribution
			adaptable="true"
			id="com.liferay.ide.server.ui.popup.opengogoshell"
			objectClass="org.eclipse.wst.server.core.IServer">
			<visibility>
				<and>
					<objectState
						name="serverType"
					 value="com.liferay.ide.server.portal">
					</objectState>
				</and>
			</visibility>
			<action
				class="com.liferay.ide.server.ui.action.OpenGogoShellAction"
				enablesFor="1"
				icon="icons/e16/osgi.png"
				id="com.liferay.ide.server.ui.action.openGogoShell"
			   label="Open Gogo Shell">
			</action>
		</objectContribution>
	</extension>
	<extension
