			commandId="com.liferay.ide.server.ui.handlers.redeploy"
			contextId="org.eclipse.ui.serverViewScope"
			schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
			sequence="M1+M2+R">
		</key>
		<key
			commandId="com.liferay.ide.server.ui.handlers.openLiferayHomeFolder"
			contextId="org.eclipse.ui.serverViewScope"
